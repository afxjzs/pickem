{
	"buildCommand": "npm run build",
	"devCommand": "npm run dev",
	"installCommand": "npm install",
	"framework": "nextjs",
	"regions": ["iad1"],
	"crons": [
		{
			"path": "/api/cron/sync-scores",
			"schedule": "*/5 * * * *"
		},
		{
			"path": "/api/cron/sync-schedules",
			"schedule": "0 0 * * 0"
		},
		{
			"path": "/api/cron/sync-odds",
			"schedule": "0 * * * *"
		}
	]
}

